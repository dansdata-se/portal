{
  "name": "portal",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prestart": "npm run gen",
    "start": "flutter run -d web-server --web-hostname 0.0.0.0 --web-port 3000",
    "build": "flutter build web --release --dart-define=\"NEXT_PUBLIC_VERCEL_URL=$NEXT_PUBLIC_VERCEL_URL\" --dart-define=\"NEXT_PUBLIC_SUPABASE_URL=$NEXT_PUBLIC_SUPABASE_URL\" --dart-define=\"NEXT_PUBLIC_SUPABASE_ANON_KEY=$NEXT_PUBLIC_SUPABASE_ANON_KEY\" --dart-define=\"NEXT_PUBLIC_VERCEL_ENV=$NEXT_PUBLIC_VERCEL_ENV\" --dart-define=\"NEXT_PUBLIC_VERCEL_GIT_COMMIT_REF=$NEXT_PUBLIC_VERCEL_GIT_COMMIT_REF\" --dart-define=\"NEXT_PUBLIC_VERCEL_GIT_COMMIT_SHA=$NEXT_PUBLIC_VERCEL_GIT_COMMIT_SHA\"",
    "pretest": "npm run gen",
    "test": "flutter test",
    "gen": "flutter packages pub run build_runner build",
    "lint": "flutter analyze && tbls lint",
    "fix": "dart fix --apply && npm run format",
    "format": "prettier --write . && flutter format lib/ && find ./supabase/ -type f -name '*.sql' -exec pg_format -c ./.pg_format -i {} \\;",
    "format-check": "prettier --check . && flutter format --set-exit-if-changed -o none lib/",
    "prepare": "husky install && flutter pub get && npm run gen",
    "docs": "tbls doc --rm-dist"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "env-cmd": "^10.1.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5"
  }
}
